<script>
    window.GujAd = window.GujAd || {};
    GujAd.cmd = GujAd.cmd || [];

    (function () {
        var deferredElement = document.querySelectorAll('#recipe-image-carousel .recipe-image-carousel-slide');
        var lighboxRecipeSlider = document.querySelector('#recipe-image-lightbox-carousel');
        var ads = {
            initialized: false,
            slideChangeCount: 0,
            reload: async function () {
                ads.slideChangeCount = 0;

                document
                    .querySelectorAll('guj-ad[position=superbanner_2], guj-ad[position=mobile_5], guj-ad[position=skyscraper_2]')
                    .forEach(adSlot => adSlot.reload())
            }
        }

        var ivw = {
            sendRequest: async function() {
                                if (typeof window.ck?.recipe?.sendIomPageview === 'function') {
                    window.ck.recipe.sendIomPageview()
                }
            }
        };

        Array.prototype.map.call(deferredElement, function(element) {
            element.addEventListener('click', function (event) {
                // Set to -1, because the opening is the first slide change...
                ads.slideChangeCount = -1;
                if (!ads.initialized) {
                    ads.initialized = true;
                } else {
                    ads.reload();
                }
                ivw.sendRequest();
                                try {
                    document.querySelector('agf-metadata').sendWPI()
                } catch (e) {
                    console.error('agf-metadata component not initialized', e)
                }
            });
        });

        lighboxRecipeSlider.addEventListener('slideChange', function () {
            if (ads.slideChangeCount < 2) {
                ads.slideChangeCount++;
                return;
            }
            ads.reload();
            ivw.sendRequest();
                    });
    })();
</script>

